                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Page Number",
                        "name": "page",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Movies Information",
                        "schema": {
                            "$ref": "#/definitions/controllers.MoviesListResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid user_id"
                    },
                    "500": {
                        "description": "Internal server error"
